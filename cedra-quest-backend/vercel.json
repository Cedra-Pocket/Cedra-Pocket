{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SKIP_ENV_VALIDATION": "true"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci --ignore-scripts"
}